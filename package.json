    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-imperative": "^3.0.0",
    "eslint-plugin-imperative": "^3.0.0",
    "hath": "^1.0.1",
    "hath-assert": "^0.2.0",
    "hath-report-spec": "^0.1.0",
    "husky": "^4.2.3",
    "nyc": "^15.0.1"
  },
  "keywords": [
    "marv",
