{
  "name": "marv",
  "version": "2.1.0",
  "description": "A programmatic database migration tool with plugable drivers for mysql, sqlite and postgres",
  "main": "index.js",
  "scripts": {
    "test": "node test/suite.js",
    "lint": "eslint .",
    "istanbul": "istanbul cover --report html --report lcov test/suite.js",
    "codeclimate": "(test -e .codeclimate && npm run istanbul && source .codeclimate) || echo skipping codeclimate",
    "prepush": "npm run lint && npm test && npm run codeclimate"
  },
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "dependencies": {
    "async": "^2.6.0",
    "debug": "^4.0.0",
    "lodash": "^4.17.10",
    "xregexp": "^4.1.1"
  },
  "devDependencies": {
    "eslint": "^5.5.0",
    "eslint-config-imperative": "^2.1.0",
    "eslint-plugin-imperative": "^2.1.0",
    "hath": "^1.0.1",
    "hath-assert": "^0.2.0",
    "husky": "^0.14.3",
    "istanbul": "^0.4.5"
  },
  "keywords": [
    "marv",
    "database",
    "db",
    "migration",
    "migrate",
    "postgres",
    "postgresql",
    "mysql",
    "sqlite",
    "sqlite3"
  ],
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/marv.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/marv/issues"
  },
  "homepage": "https://guidesmiths.github.io/marv/"
