{
  "name": "marv",
  "version": "2.3.1",
  "description": "A programmatic database migration tool with plugable drivers for mysql, postgres, mssql and sqlite",
  "main": "index.js",
  "scripts": {
    "test": "node test/suite.js",
    "lint": "eslint .",
    "coverage": "nyc --report html --reporter lcov --reporter text-summary node test/suite.js",
    "codeclimate": "(test -e .codeclimate && npm run coverage && source .codeclimate) || echo skipping codeclimate"
  },
  "author": "GuideSmiths Ltd",
  "license": "ISC",
  "dependencies": {
    "async": "^3.1.0",
    "debug": "^4.1.1",
    "lodash": "^4.17.15",
    "util.promisify": "^1.0.0",
    "xregexp": "^4.2.4"
  },
  "devDependencies": {
    "eslint": "^6.1.0",
    "eslint-config-imperative": "^3.0.0",
    "eslint-plugin-imperative": "^3.0.0",
    "hath": "^1.0.1",
    "hath-assert": "^0.2.0",
    "hath-report-spec": "^0.1.0",
    "husky": "^3.0.2",
    "nyc": "^14.1.1"
  },
  "keywords": [
    "marv",
    "database",
    "db",
    "migration",
    "migrate",
    "postgres",
    "postgresql",
    "mysql",
    "sqlite",
    "sqlite3",
    "mssql"
  ],
  "directories": {
    "test": "test"
  },
  "engines": {
    "node": ">6.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/guidesmiths/marv.git"
  },
  "bugs": {
    "url": "https://github.com/guidesmiths/marv/issues"
  },
  "homepage": "https://guidesmiths.github.io/marv/",
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run codeclimate"
    }
  }
}
